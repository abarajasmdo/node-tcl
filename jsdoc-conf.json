{
	"tags" : {
		"allowUnknownTags" : true,
		"dictionaries" : [ "jsdoc", "closure" ]
	},
	"source": {
		"include" : [ "index.js", "lib/" ],
		"includePattern" : ".+\\.js(doc)?$",
		"excludePattern" : "(^|\\/|\\\\)_"
	},
	"plugins" : [],
	"templates" : {
		"cleverLinks" : false,
		"monospaceLinks" : false
	},
	"opts" : {
		"destination" : "./docs/",
		"encoding" : "utf8",
		"recurse" : true
	}
}
